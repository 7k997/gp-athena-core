{
    "name": "altv-athena",
    "version": "5.3.2",
    "description": "a roleplay framework for alt:V",
    "author": "stuyk",
    "type": "module",
    "scripts": {
        "[-] Server Deployment Commands (They All Do Different Things)": "",
        "windows": "node ./scripts/runtime/index.js --start",
        "linux": "node ./scripts/runtime/index.js --start",
        "devtest": "node ./scripts/runtime/index.js --start --devtest",
        "dev": "node ./scripts/runtime/index.js --start --dev",
        "[-] CDN Scripts": "",
        "JustPackCDN": "node ./scripts/runtime/index.js --start --cdn",
        "JustPackDevCDN": "node ./scripts/runtime/index.js --start --cdn --dev",
        "JustPackDevTestCDN": "node ./scripts/runtime/index.js --start --cdn --devtest",
        "StartLocalCDN": "node ./scripts/runtime/index.js --localCDN",
        "windowsCDN": "node ./scripts/runtime/index.js --start --localCDN",
        "linuxCDN": "node ./scripts/runtime/index.js --start --localCDN",
        "devCDN": "node ./scripts/runtime/index.js --start --dev --localCDN",
        "devtestCDN": "node ./scripts/runtime/index.js --start --devtest --localCDN",
        "[-] Updating, Installing, Cleanup": "",
        "update": "npx altv-pkg d release",
        "check": "tsc -p ./tsconfig.json | node ./scripts/fileChecker/index.js",
        "fix": "node ./scripts/doctor/index.js",
        "[-] Vue WebView Deployment": "",
        "vue-dev": "node ./scripts/plugins/webview.js && node ./scripts/plugins/files.js && npx vite ./src-webviews --clearScreen=false --host=localhost --port=3000",
        "[-] Utility": "",
        "docs": "npx typedoc --options ./src/core/typedoc.json && node ./scripts/documentation/index.js",
        "fix-natives": "node ./scripts/natives-upgrade/index.js",
        "test": "",
        "gpitemsgenerate": "node ./scripts/gpitemsgenerate/index.js",
        "gpitemsgenerateWheels": "node ./scripts/gpitemsgenerate/indexWheels.js",
        "gpitemsgenerateObject": "node ./scripts/gpitemsgenerate/indexObjects.js",
        "gpitemsgenerateClothes": "node ./scripts/gpitemsgenerate/indexClothes.js",
        "openAIGenerator": "node ./scripts/gpitemsgenerate/openAIGenerator.js",
        "translationGenerator": "node ./scripts/gpitemsgenerate/indexTranslations.js",
        "itemTranslationGenerator": "node ./scripts/gpitemsgenerate/indexItemTranslations.js",
        "indexTTS": "node ./scripts/gpitemsgenerate/indexTTS.js"
    },
    "devDependencies": {
        "@altv/types-client": "^3.0.3",
        "@altv/types-natives": "^1.5.4",
        "@altv/types-server": "^3.1.1",
        "@altv/types-shared": "^1.6.6",
        "@altv/types-webview": "^1.0.7",
        "@altv/types-worker": "1.0.7",
        "@babel/types": "^7.17.0",
        "@swc/cli": "0.1.62",
        "@swc/core": "1.3.92",
        "@types/minimatch": "^3.0.5",
        "@types/msgpack-lite": "^0.1.9",
        "@types/node": "^14.6.1",
        "@types/sockjs": "^0.3.33",
        "@types/sockjs-client": "^1.5.1",
        "altv-pkg": "^2.3.1",
        "autoprefixer": "^10.4.17",
        "fkill": "^8.1.1",
        "glob": "^10.3.1",
        "postcss": "^8.4.33",
        "prettier": "^2.6.2",
        "stylus": "^0.58.1",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.2.2",
        "vite": "^4.5.0",
        "vue": "^3.3.6"
    },
    "dependencies": {
        "@ffmpeg-installer/ffmpeg": "^1.1.0",
        "@formkit/auto-animate": "^0.8.0",
        "@iarna/toml": "^2.2.5",
        "@stuyk/ezmongodb": "3.0.1",
        "@twurple/api": "^6.1.1",
        "@twurple/auth": "^6.1.1",
        "@twurple/chat": "^6.1.1",
        "@twurple/eventsub-ws": "^6.1.1",
        "@twurple/pubsub": "^6.1.1",
        "@vitejs/plugin-vue": "^4.4.0",
        "axios": "^1.6.7",
        "bip39": "^3.1.0",
        "discord.js": "^14.14.1",
        "elliptic": "6.5.4",
        "express": "^4.18.3",
        "fluent-ffmpeg": "^2.1.2",
        "fs": "^0.0.1-security",
        "fs-extra": "^11.2.0",
        "gsap": "^3.11.5",
        "image-js": "^0.35.3",
        "jimp": "^0.3.5",
        "json2typescript": "^1.5.1",
        "mongodb": "^4.16.0",
        "msgpack-lite": "^0.1.26",
        "njwt": "^2.0.0",
        "openai": "^4.28.0",
        "path": "^0.12.7",
        "pinia": "^2.0.35",
        "sass": "^1.61.0",
        "shortid": "^2.2.16",
        "sjcl": "1.0.8",
        "sockjs": "0.3.21",
        "sockjs-client": "1.5.2",
        "vue-advanced-chat": "^2.0.10",
        "vue3-carousel": "^0.3.1",
        "vue3-colorpicker": "^2.2.3",
        "xlsx": "^0.18.5",
        "youtube-player": "^5.6.0"
    },
    "dependenciesComments": {
        "mongodb": "^4.16.0 This is already installed by ezmongodb, but it is required for the typescript compiler. Check version numbers to make sure they match."
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 120,
        "singleQuote": true,
        "semi": true,
        "arrowParens": "always",
        "trailingComma": "all"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
